                      </object>
                    </child>
                    <child type="suffix">
                      <object class="PanelToggleButton">
                        <property name="dock">dock</property>
                        <property name="area">bottom</property>
                      </object>
                    </child>
                    <child type="suffix">
                      <object class="GtkToggleButton" id="frame_header_bar">
