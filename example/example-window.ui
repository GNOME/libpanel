                                <child>
                                  <object class="GtkBox">
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkEntry">
                                        <property name="width-chars">5</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkButton">
                                        <property name="label">Go</property>
