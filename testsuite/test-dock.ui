<interface>
  <object class="GtkWindow" id="window">
    <child type="titlebar">
      <object class="GtkHeaderBar">
        <child>
          <object class="PanelDockSwitcher">
            <property name="dock">dock</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="PanelDock" id="dock">
        <property name="reveal-start">true</property>
        <property name="reveal-end">true</property>
        <property name="reveal-top">true</property>
        <property name="reveal-bottom">true</property>
        <child type="start">
          <object class="PanelWidget">
            <property name="title">Start</property>
            <property name="child">
              <object class="GtkLabel">
                <property name="label">start</property>
              </object>
            </property>
          </object>
        </child>
        <child type="top">
          <object class="PanelWidget">
            <property name="title">Top</property>
            <property name="child">
              <object class="GtkLabel">
                <property name="label">top</property>
              </object>
            </property>
          </object>
        </child>
        <child type="end">
          <object class="PanelWidget">
            <property name="can-maximize">True</property>
            <property name="title">End</property>
            <property name="child">
              <object class="GtkLabel">
                <property name="label">end</property>
                <property name="selectable">true</property>
              </object>
            </property>
          </object>
        </child>
        <child type="end">
          <object class="PanelWidget">
            <property name="title">End Two</property>
            <property name="child">
              <object class="GtkLabel">
                <property name="label">end two</property>
              </object>
            </property>
          </object>
        </child>
        <child type="bottom">
          <object class="PanelWidget">
            <property name="title">Bottom</property>
            <property name="child">
              <object class="GtkLabel">
                <property name="label">bottom</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="PanelGrid">
            <child>
              <object class="PanelWidget">
                <property name="title">Page 1.txt</property>
                <property name="icon-name">x-office-document-symbolic</property>
                <child>
                  <object class="GtkScrolledWindow">
                    <child>
                      <object class="GtkTextView">
                        <property name="buffer">
                          <object class="GtkTextBuffer">
                            <property name="text">Page 1</property>
                          </object>
                        </property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
